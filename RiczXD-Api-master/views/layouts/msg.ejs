<% if (success_msg != '') { %>
    <div class="col-md-6">
        <div class="alert  alert-success alert-dismissible fade show" role="alert">
            <%- success_msg %>
        </div>
    </div>
<% } %>

<% if (error_msg != '') { %>
    <div class="col-md-6">
        <div class="alert alert-danger" role="alert">
            <%- error_msg %>
        </div>
    </div>
<% } %>